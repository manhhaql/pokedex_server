{   
    "signup": {
        "post":{
            "tags":[
               "auth"
            ],
            "summary":"Signup API",
            "description":"",
            "operationId":"signup",
            "consumes": [
                "application/x-www-form-urlencoded"
             ],
            "produces":[
               "application/json"
            ],
            "parameters":[
               {
                  "name":"type",
                  "in":"formData",
                  "description": "User type: 1-admin, 2-customer",
                  "type":"integer",
                  "format":"int32",
                  "required": true
               },
               {
                  "name":"email",
                  "in":"formData",
                  "description": "Type in your email",
                  "type":"string",
                  "required": false
               },
               {
                  "name":"username",
                  "in":"formData",
                  "description": "Type in your username",
                  "type":"string",
                  "required": true
               },
               {
                  "name":"password",
                  "in":"formData",
                  "description": "Type in your password",
                  "type":"string",
                  "required": true
               },
               {
                  "name":"password_check",
                  "in":"formData",
                  "description": "Type in your password check",
                  "type":"string",
                  "required": true
               }
            ],
            "responses":{
               "200":{
                  "description":"OK",
                  "schema":{
                    "properties": {
                        "code": {
                            "$ref": "../definitions/common.json#/StatusCode"
                        },
                        "error": {
                            "$ref": "../definitions/common.json#/ErrorObject"
                        },
                        "data": {
                            "$ref": "#/definitions/AuthenticationData"
                        }
                    }
                  }
               },
               "400":{
                  "description":"Invalid input value"
               },
               "404":{
                  "description":"Data not found"
               }
            }
        }
    },
